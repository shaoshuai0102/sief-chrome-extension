{
  "manifest_version": 2,
  "name": "sief",
  "description": "Extension for sief. https://github.com/shaoshuai0102/thief",
  "version": "0.1.0",
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": ["socket.io-1.1.0.js", "background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}